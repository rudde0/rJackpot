							message "{@prefix}Hızlı Pota en fazla &6{@fasterPotMaxKisi} kişi &7katılabilmektedir!"
							stop
						if player's balance is higher than {@fasterPotBiletPara}*arg 2:
							if {katilimFaster.%player%} is higher than {@fasterPotMaxGiris}:
								message "{@prefix}fasterPota en fazla &6{@fasterPotMaxGiris} &7bilet ekleme girişiminde bulunabilirsin!"
								stop
							add arg 2 to {yatirilanBiletFaster.%player%}
							loop arg 2 times:
								add player to {biletListeFaster::*}
							add arg 2 to {ticketSansBiletleriFaster.%player%}
							add {@fasterPotBiletPara}*arg 2 to {mevcutPotFaster}
							add {@fasterPotBiletPara}*arg 2 to {yatirilanMiktarFaster.%player%}
							subtract {@fasterPotBiletPara}*arg 2 from {potKazancFaster.%player%}
							subtract {@fasterPotBiletPara}*arg 2 from player's balance
							if {katilimFaster.%player%} is smaller than {@fasterPotMaxGiris}:
								add 1 to {katilimFaster.%player%}
							if {katilmistimFaster.%player%} isn't set:
								add player to {katilanlarFaster::*}
								set {katilmistimFaster.%player%} to on
							add arg 2 to {FasterguncelBiletSayisi.%player%}
							if {@biletDelay} is on:
								if {FasterbiletParaDelayda.%player%} is not set:
									if {@delaydaveMesajTeslimEt} is on:
										message "{@prefix}Hızlı Pota yatırdığınız biletler sisteme işlendi, birazdan bilgi mesajı geçilecek."
									set {FasterbiletParaDelayda.%player%} to on
									wait 5 seconds
									broadcast "{@prefix}&e%player% &7Hızlı Pota &6+%{FasterguncelBiletSayisi.%player%}% bilet &7ekledi!"
									broadcast "&7Hızlı Pot devredilecek miktar: &6$%{mevcutPotFaster}%"
									delete {FasterguncelBiletSayisi.%player%}
									delete {FasterbiletParaDelayda.%player%}
							else:
								broadcast "{@prefix}&e%player% &7Hızlı Pota &6+%arg 2% bilet &7ekledi!"
								broadcast "&7Hızlı Pot devredilecek miktar: &6$%{mevcutPotFaster}%"
						else:
							message "{@prefix}&6+%arg 2% &7bilet eklemek için &6$%{@fasterPotBiletPara} * arg 2% &7paran olmalı."
					if arg 3 is "fastest":
						if {PotBakim} is set:
							if player doesn't have permission "{@adminPanel}":
								message "{@prefix}Jackpot girişleri kapalı gözüküyor!"
								stop
						if arg 2 is higher than {@fastestPotMaxGiris}:
							message "{@prefix}Çok Hızlı Pota tek seferde en fazla &6{@fastestPotMaxGiris} &7bilet satın alabilirsin!"
							stop
						if {buTetiklendi} is set:
							message "{@prefix}Çok Hızlı Pot birazdan döneceği için para yatıramıyorsunuz."
							stop
						if player's balance is higher than {@fastestPotBiletPara}*arg 2:
							if {katilimFastest.%player%} is higher than {@fastestPotMaxGiris}:
								message "{@prefix}Çok Hızlı Pot en fazla &6{@fastestPotMaxGiris} &7bilet ekleme girişiminde bulunabilirsin!"
								stop
							add arg 2 to {yatirilanBiletFastest.%player%}
							loop arg 2 times:
								add player to {biletListeFastest::*}
							add arg 2 to {ticketSansBiletleriFastest.%player%}
							add {@fastestPotBiletPara}*arg 2 to {mevcutPotFastest}
							add {@fastestPotBiletPara}*arg 2 to {yatirilanMiktarFastest.%player%}
							subtract {@fastestPotBiletPara}*arg 2 from {potKazancFastest.%player%}
							subtract {@fastestPotBiletPara}*arg 2 from player's balance
							if {katilimFastest.%player%} is smaller than {@fastestPotMaxGiris}:
								add 1 to {katilimFastest.%player%}
							if {katilmistimFastest.%player%} isn't set:
								add player to {katilanlarFastest::*}
								set {katilmistimFastest.%player%} to on
								add 1 to {katilimKontrol}
							add arg 2 to {FastestguncelBiletSayisi.%player%}
							if {@biletDelay} is on:
								if {FastestbiletParaDelayda.%player%} is not set:
									if {@delaydaveMesajTeslimEt} is on:
										message "{@prefix}Çok Hızlı Pota yatırdığınız biletler sisteme işlendi, birazdan bilgi mesajı geçilecek."
									set {FastestbiletParaDelayda.%player%} to on
									set {SiradaBiriVar} to on
									wait 5 seconds
									broadcast "{@prefix}&e%player% &7Çok Hızlı Pota &6+%{FastestguncelBiletSayisi.%player%}% bilet &7ekledi!"
									broadcast "&7Çok Hızlı Pot devredilecek miktar: &6$%{mevcutPotFastest}%"
									delete {FastestguncelBiletSayisi.%player%}
									delete {FastestbiletParaDelayda.%player%}
									delete {SiradaBiriVar}
							else:
								broadcast "{@prefix}&e%player% &7Çok Hızlı Pota &6+%arg 2% bilet &7ekledi!"
								broadcast "&7Çok Hızlı Pot devredilecek miktar: &6$%{mevcutPotFastest}%"
							if {katilimKontrol} is higher than {@fastestPotMaxKisi}-1:
								if {buTetiklendi} is not set:
									if {SiradaBiriVar} is not set:
										execute console command "drawfastestkonsol"
										set {buTetiklendi} to on
										stop
									else:
										set {buTetiklendi} to on
										broadcast "{@prefix}Çok Hızlı Pota şu an birisi para yatırdığı için devretmesi birkaç saniye ertelendi."
										wait 5 seconds
										execute console command "drawfastestkonsol"
										stop
			else if arg 1 is "sifirla" or "sıfırla":
				if player has permission "{@adminPanel}":
					message "{@prefix}Tüm bilgiler sıfırlandı!"
					set {mevcutPot} to 0
					clear {katilanlar::*}
					loop all players:
						clear {yatirilanMiktar.%loop-player%}
						clear {katilmistim.%loop-player%}
					broadcast "{@prefix}Jackpota katılmak için &a/jackpot &7yazabilirsin!"
					set {kalanDk} to {@PotDakika}
					set {kalanSn} to 1
				else:
					message "{@prefix}Komutu kullanmak için gerekli yetkiniz bulunmuyor!"
			else if arg 1 is "sohbet":
				if player has permission "{@adminPanel}":
					clear {sohbetJackpot}
					message "{@prefix}Başarıyla sohbet açıldı."
			else if arg 1 is "ekstra":
				if player has permission "{@adminPanel}":
					if arg 2 is not set:
						execute player command "/jackpot"
					else:
						broadcast "{@prefix}&e%player% &7{@classicPot}'a ekstra &6$%arg 2% &7ekledi!"
						add arg 2 to {mevcutPot}
				else:
					message "{@prefix}Komutu kullanmak için gerekli yetkiniz bulunmuyor!"
			else if arg 1 is "ekstrafaster":
				if player has permission "{@adminPanel}":
					if arg 2 is not set:
						execute player command "/jackpot"
					else:
						broadcast "{@prefix}&e%player% &7Hızlı Pot'a ekstra &6$%arg 2% &7ekledi!"
						add arg 2 to {mevcutPotFaster}
				else:
					message "{@prefix}Komutu kullanmak için gerekli yetkiniz bulunmuyor!"
			else if arg 1 is "ekstrafastest":
				if player has permission "{@adminPanel}":
					if arg 2 is not set:
						execute player command "/jackpot"
					else:
						broadcast "{@prefix}&e%player% &7Çok Hızlı Pot'a ekstra &6$%arg 2% &7ekledi!"
						add arg 2 to {mevcutPotFastest}
				else:
					message "{@prefix}Komutu kullanmak için gerekli yetkiniz bulunmuyor!"
			else if arg 1 is "drawfastest":
				if player doesn't have permission "{@adminPanel}":
					message "{@prefix}Komutu kullanmak için gerekli yetkiniz bulunmuyor!"
				else:
					execute console command "drawfastestkonsol"
					
command /drawfastestkonsol:
	executable by: players and console
	permission: {@adminPanel}
	permission message: Bu komut sadece konsol üzerinden kullanılabilir.
	trigger:
		if {@guiSistem} is off:
			message "{@prefix}Bu özellik &6guiSistem &7kapalıyken çalışmamaktadır."
		else:
			if {katilanlarFastest::*} is smaller than {@fastestPotMinKisi}:
				message "{@prefix}Çok Hızlı Pot'a yeterli kişi katılmamış."
				stop
			if {cekilmekte} is on:
				broadcast "{@prefix}Halihazırda bir Jackpot çekiliyor olduğu için, Çok Hızlı Pot biraz sonra birden dönecek!"
				wait 30 seconds
				execute console command "jackpot drawfastest"
				stop
			broadcast "{@prefix}Şu an Çok Hızlı Pot sonuçları işleniyor, birazdan kazanan duyurulacak!"
			wait 10 seconds
			clear {kazananBiletHashFastest::*}
			loop {biletListeFastest::*}:
				add 1 to {RoundSayiFastest::%loop-value%}
				set {biletListeFastest::KazananFastest} to random element of {biletListeFastest::*}
			add 1 to {fastestPotSayi}
			set {_sans} to {yatirilanMiktarFastest.%{biletListeFastest::KazananFastest}%}/{mevcutPotFastest}*100
			if {@actionBarMesajlari} is on:
				loop 3 times:
					set action bar of all players to coloured "&6&lÇok Hızlı Pot» &7Kazanan kişi &6&o%{biletListeFastest::KazananFastest}% &7tebrikler! &6($%{mevcutPotFastest}%)"
					wait 2 second
			broadcast "&8&l&m---------------------------------------------"
			broadcast " "
			broadcast "                         &r&6&l» &c&lÇok Hızlı JackPot &6&l«"
			broadcast "                         &7&lKAZANAN &6&l» &7&l%{biletListeFastest::KazananFastest}%"
			broadcast "   &7Kazandığı Para &6- &7$%{mevcutPotFastest}%    &7Yatırdığı Para &6- &7$%{yatirilanMiktarFastest.%{biletListeFastest::KazananFastest}%}%"
			broadcast "                           &7Toplam Kâr &6- &7$%{mevcutPotFastest}-{yatirilanMiktarFastest.%{biletListeFastest::KazananFastest}%}%"
			broadcast "                               &7Şans &6- &7&l%%&7%{_sans}%"
			broadcast " "
			broadcast "&6(##%{fastestPotSayi}%&6) &7Bu Çok Hızlı Pot rounduna katılanlar: &7%{katilanlarFastest::*}%"
			broadcast " "
			broadcast "&8&l&m---------------------------------------------"
			log "(##%{fastestPotSayi}% - FASTEST) rJackpot > %{biletListeFastest::KazananFastest}% adlı kişi, $%{mevcutPotFastest}% kazandı!" to "OlayLoglari.log"
			add {mevcutPotFastest} to {biletListeFastest::KazananFastest}'s balance
			add {mevcutPotFastest} to {potKazanc.%{biletListeFastest::KazananFastest}%}
			add {mevcutPotFastest} to {toplamKazanc.%{biletListeFastest::KazananFastest}%}
			set {GecmisKazananFastest} to {biletListeFastest::KazananFastest}
			set {GecmisPotFastest} to {mevcutPotFastest}
			set {GecmisSansFastest} to {_sans}
			set {biletListeFastest::KazananFastest} to "&6%{biletListeFastest::KazananFastest}%&7"
			add {biletListeFastest::*} to {kazananBiletHashFastest::*}
			set {mevcutPotFastest} to 0
			clear {katilanlarFastest::*}
			clear {biletListeFastest::*}
			clear {biletListeFastest::KazananFastest}
			clear {katilanKisilerFastest}
			clear {CekiliyorFastest}
			clear {PotDolduFastest}
			clear {PotKapandiFastest}
			clear {sohbetJackpot}
			clear {katilimKontrol}
			delete {buTetiklendi}
			loop all players:
				clear {yatirilanMiktarFastest.%loop-player%}
				clear {katilimFastest.%loop-player%}
				clear {sonucFastest.%loop-player%}
				clear {ticketToplamYuzdeFastest.%loop-player%}
				clear {katilimFastest.%loop-player%}
				clear {yatirilanHatirlatFastest.%loop-player%}
				clear {yatirilanBiletFastest.%loop-player%}
				clear {katilmistimFastest.%loop-player%}
				clear {ticketSansBiletleriFastest.%loop-player%}
						
command /{@classicPot}:
	trigger:
		if {@guiSistem} is off:
			message "{@prefix}Bu özellik &6guiSistem &7kapalıyken çalışmamaktadır."
		else:
			if {ticketSansBiletleri.%player%} is larger than 0:
				loop all players:
					if {ticketSansBiletleri.%loop-player%} is larger than 0:
						add {ticketSansBiletleri.%loop-player%} to {ticketToplamYuzde.%player%}
				add {ticketSansBiletleri.%player%}*100/{ticketToplamYuzde.%player%} to {sonuc.%player%}
			if player has permission "{@adminPanel}":
				open virtual chest with size 4 named "     &9&lJackpot » {@classicPot}" to player
			else:
				open virtual chest with size 3 named "     &9&lJackpot » {@classicPot}" to player
			make gui slot 0 of player with {@glassSusleme} named "&6{@glassBaslik}" to nothing
			loop 36 times:
				make gui slot 0+loop-value of player with {@glassSusleme} named "&6{@glassBaslik}" to nothing
			if {katilanlar::*} isn't set:
				make gui slot 2 of player with sign item named "&6&lKatılanlar" with lore "&6Bu Jackpot rounduna kimse katılmamış!" to nothing
			else:
				make gui slot 2 of player with sign item named "&6&lKatılanlar" with lore "&6Bu Jackpot rounduna katılan oyuncular:||||&7%{katilanlar::*}%" to nothing
			if {katilanlar::*} isn't set:
				make gui slot 4 of player with chest named "&6&lMevcut Durum" with lore "&6Daha kimse katılmadığı için pot bilgisi yok." to nothing
			else:
				if {katilmistim.%player%} is set:
					set {_sans} to {yatirilanMiktar.%player%}/{mevcutPot}*100
					make gui slot 4 of player with chest named "&6&lMevcut Durum" with lore "&6Devredilecek para: &7$%{mevcutPot}%||&6Round numarası: &7##%{classicPotSayi} + 1%||&6Yatırdığın para: &7$%{yatirilanMiktar.%player%}%||&6Kazanma şansın: &7%{_sans}%%%" to nothing
				else:
					make gui slot 4 of player with chest named "&6&lMevcut Durum" with lore "&6Devredilecek para: &7$%{mevcutPot}%||&6Round numarası: &7##%{classicPotSayi} + 1%" to nothing
			if {GecmisKazanan} isn't set:
				make gui slot 12 of player with map named "&6&lGeçmiş Round" with lore "&7&l##Bilinmeyen &7numaralı oyuna ait bilgiler:||||&7Kazanan &6&l> &7Geçmiş kazanan bilgisi yok||&7Devredilen Para &6&l> &7Geçmiş round bilgisi yok||||&7Hash:||&7Geçmiş round bilgisi yok" to nothing
			else:
				make gui slot 12 of player with map named "&6&lGeçmiş Round" with lore "&7&l##%{classicPotSayi}% &7numaralı oyuna ait bilgiler:||&7Kazanan &6&l> &7%{GecmisKazanan}%||&7Devredilen Para &6&l> &7%{GecmisPot}%||&7Şans &6&l> &7%{GecmisSans}%%%||||&7Hash: &6Bilgiye ulaşmak için yazıya tıklayın." to run:
					message "&7Geçmiş oyunun bilet numaraları ve kazanan bilet:"
					message "&7%{kazananBiletHash::*}%"
			if {katilanlar::*} isn't set:
				make gui slot 6 of player with redstone named "&6&lKalan Süre" with lore "&6Daha kimse katılmadığı için süre başlatılmadı." to nothing
			else:
				make gui slot 6 of player with clock named "&6&lKalan Süre" with lore "&6%{KalanDk}% &7dakika &6%{KalanSn}% &7saniye sonra para devredilecek!" to nothing
			make gui slot 18 of player with diamond named "&6&lPara ekle" with lore "&7Buraya tıklayarak pota &6+1 bilet &7yatırırsın.||||&6Not: &7Jackpota hızlı bilet eklemek için||&6/jackpot yatır (miktar) classic &7kullanabilirsin." to run player command "jackpot yatır 1 classic"
			make gui slot 19 of player with 3 diamonds named "&6&lPara ekle" with lore "&7Buraya tıklayarak pota &6+3 bilet &7yatırırsın.||||&6Not: &7Jackpota hızlı bilet eklemek için||&6/jackpot yatır (miktar) classic &7kullanabilirsin." to run player command "jackpot yatır 3 classic"
			make gui slot 20 of player with 5 diamonds named "&6&lPara ekle" with lore "&7Buraya tıklayarak pota &6+5 bilet &7yatırırsın.||||&6Not: &7Jackpota hızlı bilet eklemek için||&6/jackpot yatır (miktar) classic &7kullanabilirsin." to run player command "jackpot yatır 5 classic"
			make gui slot 26 of player with tripwire hook named "&6&lSayfayı yenile" with lore "&7Buraya tıklayarak menüyü yenileyebilirsin!" to run:
				close player's inventory
				make player execute "{@classicPot}"
			if {RoundSayi::%player%} is set:
				make gui slot 22 of player with exp bottle named "&6&lGenel İstatistik" with lore "&7Jackpotta &6%{RoundSayi::%player%}% &7round oyun oynamışsınız.||&7Jackpottan toplam kazancınız ise &6$%{toplamKazanc.%player%}%" to nothing
			else:
				make gui slot 22 of player with exp bottle named "&6&lGenel İstatistik" with lore "&7Jackpotta daha önce oynamamışsınız." to nothing
			make gui slot 24 of player with tripwire hook named "&6&lGeriye dön" with lore "&7Buraya tıklayarak önceki sayfaya dönebilirsin!" to run:
				close player's inventory
				make player execute "jackpot"
			make gui slot 25 of player with tripwire hook named "&6&lHızlı Pot" with lore "&7Buraya tıklayarak &6Hızlı Pot &7sayfasına gidebilirsin!" to run:
				close player's inventory
				make player execute "jackpotfaster"
			make gui slot 14 of player with paper named "&6&lChat Jackpot Duyuruları" with lore "&7Chatte Jackpot dolum mesajlarını tıklayarak açıp kapatabilirsin." to run player command "jackpot mesaj"
			if player has permission "{@adminPanel}":
				make gui slot 27 of player with book and quill named "&6&lHızlı Çek &9&l(ADMIN)" with lore "&7Tıklarsanız Jackpot on saniye içerisinde para devreder." to run:
					if player has permission "{@adminPanel}":
						set {kalanDk} to 0
						set {kalanSn} to 10
					else:
						message "{@prefix}Yetkiniz yok."
				make gui slot 35 of player with book and quill named "&6&lPota Para Ekle &9&l(ADMIN)" with lore "&7Tıklarsanız sunucu kasasından pota para eklenir.||&7Klik başına, &6${@klikBonus} &7eklenmektedir." to run player command "jackpot ekstra {@klikBonus}"
			clear {ticketToplamYuzde.%player%}
			clear {sonuc.%player%}
			
