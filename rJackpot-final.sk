command /jackpot [<text>] [<integer>] [<text>]:
	aliases: {@komutAliaslar}
	trigger:
		if {@guiSistem} is off:
			if arg 1 is not set:
				message "{@prefix} &6Jackpot Komutları {@prefix}"
				message "&6/jackpot ne &c-> &6Açıklama"
				message "&6/jackpot yatır [miktar] &c-> &6Bilet yatırma"
				message "&6/jackpot durum &c-> &6Şuanki pot hakkında bilgiler"
				message "&6/jackpot kazanç &c-> &6Şuana kadarki kârınız"
				message "&6/jackpot mesaj &c-> &6Chatte jackpot mesajlarını görüp görmeme ayarı"
				if player has permission "{@adminPanel}":
					message "&6/jackpot başlat &c-> &6Zamanı sıfırlar ve hemen potu başlatır"
					message "&6/jackpot ekstra [miktar] &c-> &6Pota ekstra para ekler"
					message "&6/jackpot bakım &c-> &6Jackpotu kapatır (açmak istersen tekrar yaz)"
			else if arg 1 is not "mesaj" or "ne" or "yatır" or "yatir" or "durum" or "başlat" or "baslat" or "ekstra" or "kazanc" or "kazanç" or "sifirla" or "sıfırla" or "draw" or "bakım" or "bakim" or "sohbet":
				execute player command "/jackpot"
			else if arg 1 is "mesaj":
				if {mesajlar.%player%} contains player:
					remove player from {mesajlar.%player%}
					message "{@prefix}Artık jackpot dolum mesajları almayacaksınız."
				else:
					loop 10 times:
						subtract player from {mesajlar.%player%}
					add player to {mesajlar.%player%}
					message "{@prefix}Tekrardan jackpot dolum mesajları alacaksınız."
			else if arg 1 is "ne":
				message "{@prefix}Jackpot nedir:"
				message "&7Herkes ortaya ${@PotBiletPara} değerinde biletler atar."
				message "&7ne kadar çok bilet atarsan o kadar fazla kazanma şansın olur."
				message "&7ama unutma ki %%99.99 şansla kaybedeceğin gibi %%0.01 şansla"
				message "&7kazanabilirsin."
			else if arg 1 is "yatır" or "yatir":
				if arg 2 is not set:
					execute player command "/jackpot"
				else if arg 2 is smaller than 1:
					execute player command "/jackpot"
				else:
					if player's balance is higher than {@PotBiletPara}*arg 2:
						if {@birDahaKatilabilsin} is off:
							if {katilmistim.%player%} is set:
								message "{@prefix}Round başına sadece bir seferliğinde para yatırabilirsin!"
								stop
						add arg 2 to {yatirilanBilet.%player%}
						loop arg 2 times:
							add player to {biletListe::*}
						add {@PotBiletPara}*arg 2 to {mevcutPot}
						add {@PotBiletPara}*arg 2 to {yatirilanMiktar.%player%}
						subtract {@PotBiletPara}*arg 2 from {potKazanc.%player%}
						subtract {@PotBiletPara}*arg 2 from player's balance
						add arg 2 to {guncelBiletSayisi.%player%}
						if {katilmistim.%player%} is not set:
							add player to {katilanlar::*}
							set {katilmistim.%player%} to on
						if {@biletDelay} is on:
							if {biletParaDelayda.%player%} is not set:
								set {biletParaDelayda.%player%} to on
								wait 10 seconds
								broadcast "{@prefix}&e%player% &7pota toplam &6+%{guncelBiletSayisi.%player%}% bilet &7ekledi! (Dönen para: &6$%{mevcutPot}%&7)"
								delete {guncelBiletSayisi.%player%}
								delete {biletParaDelayda.%player%}
						else:
							broadcast "{@prefix}&e%player% &7pota &6+%arg 2% bilet &7ekledi! (Dönen para: &6$%{mevcutPot}%&7)"
					else:
						message "{@prefix}&6+%arg 2% &7bilet eklemek için &6$%{@PotBiletPara} * arg 2% &7paran olmalı."
			else if arg 1 is "durum":
				message "{@prefix}Katılanlar: &6%{katilanlar::*}%"
				message "{@prefix}Devredilcek para: &6$%{mevcutPot}%"
				message "{@prefix}Çekilmesine kalan süre: &6%{kalanDk}%dakika %{kalanSn}%saniye"
				if {yatirilanMiktar.%player%} is set:
					message "{@prefix}Senin yatırdığın para: &6$%{yatirilanMiktar.%player%}%"
			else if arg 1 is "kazanç" or "kazanc":
				message "{@prefix}Pottan toplam kârınız: &6$%{potKazanc.%player%}%"
			else if arg 1 is "başlat" or "baslat":
				if player has permission "{@adminPanel}":
					message "{@prefix}Başarılı! Jackpot birazdan çekilecek."
					wait 5 seconds
					broadcast "{@prefix}Jackpot &6{@saniyeDraw} saniye &7sonra çekilecek!"
					set {kalanDk} to 0
					set {kalanSn} to {@saniyeDraw}
					set {PotKapandi} to {@saniyeDraw}
				else:
					message "{@prefix}Komutu kullanmak için gerekli yetkiniz bulunmuyor!"
			else if arg 1 is "ekstra":
				if player has permission "{@adminPanel}":
					if arg 2 is not set:
						execute player command "/jackpot"
					else:
						broadcast "{@prefix}&e%player% &7jackpota ekstra &6$%arg 2% &7ekledi!"
						add arg 2 to {mevcutPot}
				else:
					message "{@prefix}Komutu kullanmak için gerekli yetkiniz bulunmuyor!"
			else if arg 1 is "sifirla" or "sıfırla":
				if player has permission "{@adminPanel}":
					message "{@prefix}Tüm bilgiler sıfırlandı!"
					set {mevcutPot} to 0
					clear {katilanlar::*}
