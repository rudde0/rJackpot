command /jackpotfastest:
	trigger:
		if {@guiSistem} is off:
			message "{@prefix}Bu özellik &6guiSistem &7kapalıyken çalışmamaktadır."
		else:
			if player has permission "{@adminPanel}":
				open virtual chest with size 4 named "     &9&lJackpot » Çok Hızlı Pot" to player
			else:
				open virtual chest with size 3 named "     &9&lJackpot » Çok Hızlı Pot" to player
			make gui slot 0 of player with {@glassSusleme} named "&6{@glassBaslik}" to nothing
			loop 36 times:
				make gui slot 0+loop-value of player with {@glassSusleme} named "&6{@glassBaslik}" to nothing
			if {katilanlarFastest::*} isn't set:
				make gui slot 2 of player with sign item named "&6&lKatılanlar" with lore "&6Bu Çok Hızlı Pot rounduna kimse katılmamış!" to nothing
			else:
				make gui slot 2 of player with sign item named "&6&lKatılanlar" with lore "&6Bu Çok Hızlı Pot rounduna katılan oyuncular:||||&7%{katilanlarFastest::*}%" to nothing
			if {katilanlarFastest::*} isn't set:
				make gui slot 4 of player with chest named "&6&lMevcut Durum" with lore "&6Daha kimse katılmadığı için pot bilgisi yok." to nothing
